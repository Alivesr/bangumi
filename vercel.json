{
  "rewrites": [
    {
      "source": "/calendar",
      "destination": "/index.html"
    },
    {
      "source": "/subject/:id",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico).*)",
      "destination": "/index.html"
    },
    {
      "source": "/callback-success",
      "destination": "/index.html"
    }
  ]
}
